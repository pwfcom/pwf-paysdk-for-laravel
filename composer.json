{
	"name" : "pwf/laravel-pay",
	"description" : "PWF PAY SDK For Laravel",
	"type" : "library",
	"license" : "Apache-2.0",
	"version" : "1.0",
	"autoload" : {
		"psr-4" : {
			"Pwf\\LaravelPay\\" : "src/"
		}
	},
	"minimum-stability" : "dev",
	"require" : {
		"illuminate/support" : "^8.0 || ^9.0",
		"illuminate/contracts" : "^8.0 || ^9.0",
		"pwf/paysdk" : "dev-main"
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Pwf\\LaravelPay\\PayServiceProvider"
			],
			"aliases" : {
				"PwfPay" : "Pwf\\LaravelPay\\Facades\\PwfPay"
			}
		}
	}
}